{
	"presets": [
		[
			"env",
			{
				"modules": false
			}
		],
		"react",
		"stage-0"
	],
	"env": {
		"development": {
			"plugins": ["react-hot-loader/babel"]
		}
	},
	"plugins": [
		[
			"transform-runtime",
			{
				"helpers": false,
				"polyfill": false,
				"regenerator": true,
				"moduleName": "babel-runtime"
			}
		],
		[
			"import",
			{
				"libraryName": "antd",
				"style": true
			}
		]
	]
}
